<!DOCTYPE html>
<html>
<style>
    body{margin: 30px;}
    h2{
        text-align: center;
        color: chocolate;
        background: aliceblue;
        padding: 10px 0;
    }
    table{
        border: 2px solid chocolate;
        margin-bottom: 30px;
        text-align: center;
    }
    th, td{border:1px solid chocolate;}
    a{
        text-decoration: none;
        background: bisque;
        padding: 10px 20px;
        border-radius: 20px;
        font-weight: 600;
}
</style>
<body>
<h2>User Data</h2>
<table style="width:100%" cellpadding="5px" cellspacing="3px">
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Age</th>
    <th>City</th>
    <th>Profession</th>
  </tr>

  
    <% users.forEach(user =>{ %>
    <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.age %></td>
        <td><%= user.city %></td>
        <td><%= user.profession%></td>    
    </tr>
    <% }) %>
    
</table>

<center><a href= "/form">Add New User</a></center>
</body>
</html>

